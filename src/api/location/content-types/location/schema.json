      }
    },
    "phoneNumber": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "regex": "^\\+[1-9]{1}[0-9]{3,14}$"
    },
    "linkButton": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.link-button"
